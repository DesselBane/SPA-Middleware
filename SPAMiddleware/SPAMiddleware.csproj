<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>

      <PackageId>SpaMiddleware</PackageId>
      <Version>1.0.2</Version>
      <Authors>Dark Phantom</Authors>
      <Company>DarkSolutions</Company>
      <Title>Single Page Application Middleware</Title>
      <Description>A Package with an ASP Core Middleware component for working with SPA Applications.</Description>
      <PackageLicenseUrl>https://github.com/DesselBane/SPA-Middleware/blob/master/Licence.txt</PackageLicenseUrl>
      <PackageProjectUrl>https://github.com/DesselBane/SPA-Middleware</PackageProjectUrl>
      <RepositoryUrl>https://github.com/DesselBane/SPA-Middleware</RepositoryUrl>
      <RepositoryType>git</RepositoryType>
      <PackageTags>WebApi;AspNet;SPA</PackageTags>
      <IncludeSource>True</IncludeSource>
      
    
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http" Version="2.0.1" />
  </ItemGroup>
</Project>